<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tuyou.mqtt.producer.dao.ITemplateMsgMapper">
    <!--<resultMap id="BaseResultMap" type="TemplateInfoDO">-->

    <!--</resultMap>-->

    <select id="selectByEnterpriseId" resultType="com.tuyou.mqtt.producer.pojo.domain.TemplateInfoDO">
        SELECT
            id,
            enterprise_id,
            template_id,
            t.type,
            state,
            url
        FROM wechat_template_info t
        WHERE enterprise_id = #{enterpriseId} AND state = 0 AND t.type = #{type};
    </select>
</mapper>